{
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.File" ],
    "MinimumLevel": "Debug",
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "File",
        "Args": {
          "path": "%PROGRAMDATA%/FlarmTerminal/Logs/FlarmTerminal.log",
          "rollOnFileSizeLimit": true,
          "fileSizeLimitBytes": 5242880
        }
      }
    ]
  }
}

